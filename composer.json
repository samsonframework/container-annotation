{
  "name": "samsonframework/container-annotation",
  "description": "SamsonFramework annotation dependency injection container",
  "keywords": [
    "di",
    "di annotation",
    "dependency injection",
    "dependency injection annotation",
    "samsonframework di"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    },
    {
      "name": "Ruslan Molodyko",
      "email": "molodyko@samsonos.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "samsonframework/container": "*",
    "doctrine/annotations": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "samsonframework/localfilemanager": "*"
  },
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "autoload": {
    "psr-4": {
      "samsonframework\\containerannotation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "samsonframework\\containerannotation\\tests\\": "tests/"
    }
  },
  "config": {
    "github-protocols": [
      "https"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
